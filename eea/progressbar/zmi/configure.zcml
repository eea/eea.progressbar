<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  i18n_domain="eea">

  <include package="Products.CMFCore" file="permissions.zcml"/>

  <browser:page
    for="eea.progress.workflow.interfaces.IWorkflowState"
    name="manage_progress"
    class=".browser.ZMIStateProgressEdit"
    template="manage_stateprogress.pt"
    permission="cmf.ManagePortal"
    />

  <browser:page
    for="eea.progress.workflow.interfaces.IWorkflow"
    name="manage_progress"
    class=".browser.ZMIWorkflowProgressEdit"
    template="manage_progress.pt"
    permission="cmf.ManagePortal"
    />

</configure>
